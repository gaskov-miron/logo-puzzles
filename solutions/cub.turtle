kturtle-script-v1.0
@(learn) shape $run@(,) $n{
  @(repeat) 5{
  @(forward) $run @(turnright) 90
  }
  @(forward) $run / 2
  @(turnleft) 90
  @(if) $n > 1 {
  @(turnleft) 90
  @(forward) $run / 2  
  @(turnright) 45
  shape $run/1.5@(,) $n-1
  }  
  @(turnright) 90
  @(backward) $run / 2
  @(turnleft) 90
  @(backward) $run
  @(turnright) 135
  @(forward) $run / 2
  @(turnleft) 90
  }
}
@(reset)
shape 50@(,) 3